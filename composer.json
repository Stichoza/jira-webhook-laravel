{
    "name": "stichoza/jira-webhook-laravel",
    "description": "Laravel package for interacting with Jira Webhooks",
    "keywords": ["jira", "atlassian", "events", "webhook", "webhooks"],
    "type": "library",
    "require": {
        "php": "^8.2",
        "stichoza/jira-webhooks-data": "^3.0",
        "illuminate/support": "^9.18|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Stichoza\\JiraWebhooksLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Stichoza",
            "email": "me@stichoza.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Stichoza\\JiraWebhooksLaravel\\JiraWebhooksLaravelServiceProvider"
            ],
            "aliases": {
                "JiraWebhooks": "Stichoza\\JiraWebhooksLaravel\\JiraWebhooks"
            }
        }
    }
}
